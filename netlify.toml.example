## This rule redirects to an external API, signing requests with a secret
[[redirects]]
  from = "/v3/*"
  to = "https://api-football-v1.p.rapidapi.com/v3/:splat"
  status = 200
  headers = {X-RapidAPI-Key = "<RAPIDAPI_TOKEN>", X-RapidAPI-Host = "<RAPIDAPI_HOST>"}

[[redirects]]
  from = "/*"
  to = "/index.html"
  status = 200
